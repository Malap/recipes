<h1> creating new card</h1>
<%= form_for(@card) do |f| %>

<div>
	<%= f.label :name %>
	<%= f.text_field :name %>
</div>
 

<div>
	<%= f.label :category %>
	<%= f.text_field :category %>
</div>

<div>
	<%= f.label :industry %>
	<%= f.text_field :industry %>
</div>

<div>
	<%= f.submit "create software" %>
</div>
<% end %>

<p>
	<%= link_to "Back", cards_path %>
</p>